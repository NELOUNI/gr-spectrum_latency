<?xml version="1.0"?>
<block>
  <name>file_descriptor</name>
  <key>msod_sensor_file_descriptor</key>
  <category>MSOD_sensor</category>
  <import>import msod_sensor</import>
  <make>msod_sensor.file_descriptor($item_size,$fd)</make>
  <param>
    <name>Item Size</name>
    <key>item_size</key>
    <type>float</type>
  </param>
  <param>
    <name>FD</name>
    <key>fd</key>
    <type>float</type></param>
    <source>
    <name>out</name>
    <type>byte</type>
    <vlen>$num_ch</vlen>
    </source>
    <sink>
    <name>in</name>
    <type>byte</type>
    <vlen>$num_ch</vlen>
  </sink>
</block>
